package app.fastdev.util;

import org.json.JSONArray;
import org.json.JSONObject;

public class JSONUtils {

	public static JSONArray newJSONArray(String val){
		try {
			return new JSONArray(val.trim());
		} catch (Exception e) {
			return null;
		}
	}
	
	public static JSONObject newJSONObject(String val){
		try {
			return new JSONObject(val.trim());
		} catch (Exception e) {
			return null;
		}
	}
}
